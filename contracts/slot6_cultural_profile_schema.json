{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Slot 6 Cultural Profile Schema (CULTURAL_PROFILE@1)",
  "type": "object",
  "required": [
    "culture_id",
    "principle_preservation_score",
    "residual_risk"
  ],
  "additionalProperties": false,
  "properties": {
    "culture_id": {
      "type": "string",
      "minLength": 1
    },
    "principle_preservation_score": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "residual_risk": {
      "type": "number",
      "minimum": 0.0,
      "maximum": 1.0
    },
    "schema_version": {
      "type": "string",
      "enum": ["1"]
    },
    "source_slot": {
      "type": "string"
    },
    "timestamp": {
      "type": "number"
    },
    "notes": {
      "type": "string"
    }
  }
}